CREATE SCHEMA IF NOT EXISTS mydelta.manga
WITH (location = 's3a://warehouse/');

CREATE TABLE IF NOT EXISTS mydelta.manga.test (
  symbol VARCHAR,
  last_price VARCHAR,
  timestamp VARCHAR,
  volume VARCHAR,
  trade_conditions VARCHAR
)
WITH (
  external_location = 's3a://warehouse/test',
  format = 'PARQUET'
);

